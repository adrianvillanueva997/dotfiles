{
  "editor.fontLigatures": true,
  "editor.fontFamily": "'FiraCode Nerd Font', 'Fira Code', Consolas, 'Courier New', monospace",
  "terminal.integrated.fontFamily": "'FiraCode Nerd Font', 'Fira Code', Consolas, 'Courier New', monospace",
  "editor.autoClosingBrackets": "languageDefined",
  "editor.copyWithSyntaxHighlighting": true,
  // vim stuff
  "vim.useCtrlKeys": true,
  "vim.handleKeys": {
    "<C-c>": false,
    "<C-x>": false,
    "<C-v>": false,
    "<C-z>": false,
    "<C-w>": false,
    "<C-a>": false,
    "<C-f>": false,
    "<C-k>": false,
    "C-K": false
  },
  "editor.cursorBlinking": "expand",
  "editor.semanticHighlighting.enabled": true,
  "editor.minimap.renderCharacters": true,
  "editor.minimap.autohide": true,
  "editor.renderWhitespace": "selection",
  "editor.lineNumbers": "on",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "html.autoClosingTags": true,
  "terminal.integrated.gpuAcceleration": "on",
  "files.trimTrailingWhitespace": true,
  "vim.hlsearch": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.fontSize": 14,
  "workbench.tree.indent": 20,
  // Bracket pair colorizer
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": false,
  "editor.guides.highlightActiveIndentation": true,
  "editor.guides.bracketPairs": true,
  "workbench.editor.highlightModifiedTabs": true,
  "editor.guides.highlightActiveBracketPair": true,
  "editor.guides.bracketPairsHorizontal": false,
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground1": "#5bb3b3",
    "editorBracketHighlight.foreground2": "#fac863",
    "editorBracketHighlight.foreground3": "#f99157",
    "editorBracketHighlight.foreground4": "#ec5f67",
    "editorBracketHighlight.foreground5": "#bb80b3",
    "editorBracketHighlight.foreground6": "#98C379"
  },
  "editor.tokenColorCustomizations": {
    "[Kanagawa]": {
      "comments": {}
    },
    "textMateRules": [
      {
        "scope": [
          "keyword.control",
          "entity.name.type",
          "entity.name.section",
          "entity.name.tag",
          "constant"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "entity.name.function",
          "entity.name.method",
          "entity.name.class",
          "meta.class"
        ],
        "settings": {
          "fontStyle": "bold "
        }
      },
      {
        "scope": [
          "comment",
          "comment.block",
          "comment.block.documentation",
          "comment.line"
        ],
        "settings": {
          "fontStyle": "italic underline"
        }
      },
      {
        "scope": ["invalid.deprecated"],
        "settings": {
          "fontStyle": "bold strikethrough"
        }
      }
    ]
  },
  "workbench.iconTheme": "bearded-icons",
  "explorer.confirmDelete": false,
  "[markdown]": {
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.wordWrap": "on",
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    },
    "editor.formatOnSave": true
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "go.toolsManagement.autoUpdate": true,
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[html]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "telemetry.telemetryLevel": "off",
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "eslint.alwaysShowStatus": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "prettier.bracketSpacing": true,
  "prettier.arrowParens": "always",
  "prettier.embeddedLanguageFormatting": "auto",
  "eslint.run": "onType",
  "eslint.format.enable": true,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "redhat.telemetry.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "editor.stickyScroll.enabled": true,
  "terminal.integrated.smoothScrolling": true,
  "editor.inlayHints.enabled": "on",
  "python.languageServer": "Pylance",
  "python.analysis.typeCheckingMode": "basic",
  "autoDocstring.docstringFormat": "numpy",
  "autoDocstring.guessTypes": true,
  "go.vetOnSave": "package",
  "go.lintOnSave": "file",
  "go.buildOnSave": "package",
  "go.lintTool": "golangci-lint",
  "go.lintFlags": ["--fast"],
  "workbench.startupEditor": "none",
  // Python stuff
  "python.terminal.activateEnvironment": true,
  "indentRainbow.includedLanguages": ["python", "yaml", "toml", "Dockefile"],
  "indentRainbow.indicatorStyle": "light",
  "indentRainbow.colors": [
    "rgba(255, 0 , 0,0.5)",
    "rgba(255, 127, 0,0.5)",
    "rgba(255, 255, 0,0.5)",
    "rgba(0, 255, 0,0.5)",
    "rgba(0, 100, 255,0.5)",
    "rgba(100, 0, 252,0.5)",
    "rgba(250,0,211,0.5)"
  ],
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.autoSearchPaths": true,
  "python.analysis.useLibraryCodeForTypes": true,
  "python.linting.banditEnabled": true,
  "python.linting.pylintEnabled": false,
  "python.analysis.autoImportCompletions": true,
  "python.linting.mypyEnabled": false,
  "python.linting.pylamaEnabled": true,
  "python.linting.flake8Enabled": false,
  "python.linting.enabled": true,
  "python.linting.lintOnSave": true,
  "python.interpreter.infoVisibility": "onPythonRelated",
  "isort.args": ["--profile", "black"],
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true,
      "source.fixAll": true
    }
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "cSpell.enableFiletypes": ["md"],
  "cSpell.enabledLanguageIds": ["markdown"],
  "yaml.schemas": {},
  "[sql]": {
    "editor.formatOnSave": false
  },
  "javascript.autoClosingTags": true,
  "typescript.autoClosingTags": true,
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "json.schemas": [],
  "editor.inlineSuggest.enabled": true,
  "git.ignoreMissingGitWarning": true,
  "jupyter.askForKernelRestart": false,
  "editor.linkedEditing": true,
  "workbench.colorTheme": "Kanagawa",
  "workbench.preferredDarkColorTheme": "Kanagawa",
  "workbench.preferredLightColorTheme": "Horizon Extended Light",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": true,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "security.workspace.trust.untrustedFiles": "open",
  "rust-analyzer.cargo.autoreload": true,
  "rust-analyzer.checkOnSave.allTargets": true,
  "rust-analyzer.checkOnSave.enable": true,
  "rust-analyzer.completion.autoimport.enable": true,
  "rust-analyzer.diagnostics.enable": true,
  "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "always",
  "rust-analyzer.inlayHints.reborrowHints.enable": "mutable",
  "rust-analyzer.checkOnSave.command": "clippy",
  "sonarlint.rules": {
    "python:S5886": {
      "level": "off"
    },
    "python:S1515": {
      "level": "off"
    }
  },
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "workbench.productIconTheme": "fluent-icons",
  "editor.cursorSmoothCaretAnimation": true,
  "diffEditor.codeLens": true,
  "[dockercompose]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  }
}
