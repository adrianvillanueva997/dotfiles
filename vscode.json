{
  "editor.fontLigatures": true,
  "editor.fontFamily": "'FiraCode Nerd Font', 'Fira Code', Consolas, 'Courier New', monospace",
  "terminal.integrated.fontFamily": "'FiraCode Nerd Font', 'Fira Code', Consolas, 'Courier New', monospace",
  "editor.autoClosingBrackets": "languageDefined",
  "editor.copyWithSyntaxHighlighting": true,
  "vim.useCtrlKeys": true,
  "vim.handleKeys": {
    "<C-c>": false,
    "<C-x>": false,
    "<C-v>": false,
    "<C-z>": false,
    "<C-w>": false,
    "<C-a>": false,
    "<C-f>": false,
    "<C-k>": false,
    "C-K": false
  },
  // TODO: Do something cool with this
  "background.customImages": [
    "https://i.imgur.com/7kcrAQa.png",
    "https://i.imgur.com/N9t14D3.png",
    "https://i.imgur.com/O9n4fYk.png",
    "https://i.imgur.com/5VN5DrY.png",
    "https://i.imgur.com/oOagi23.png",
    "https://i.imgur.com/265wNih.png"
  ],
  "background.style": {
    "background-size": "300px 460px",
    "opacity": 0.2
  },
  "background.interval": 60,
  "editor.cursorBlinking": "expand",
  "editor.semanticHighlighting.enabled": true,
  "editor.minimap.renderCharacters": true,
  "editor.minimap.autohide": true,
  "editor.renderWhitespace": "selection",
  "editor.lineNumbers": "on",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "html.autoClosingTags": true,
  "terminal.integrated.gpuAcceleration": "on",
  "files.trimTrailingWhitespace": true,
  "vim.hlsearch": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  // Workbench tree
  "workbench.tree.indent": 15,
  "workbench.tree.renderIndentGuides": "always",
  "rust-analyzer.highlighting.strings": false,
  // Bracket pair colorizer
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": false,
  "editor.guides.highlightActiveIndentation": true,
  "editor.guides.bracketPairs": true,
  "workbench.editor.highlightModifiedTabs": true,
  "editor.guides.highlightActiveBracketPair": true,
  "editor.guides.bracketPairsHorizontal": false,
  "workbench.colorCustomizations": {
    "tree.indentGuidesStroke": "#f4f3f5",
    "editorBracketHighlight.foreground1": "#5bb3b3",
    "editorBracketHighlight.foreground2": "#fac863",
    "editorBracketHighlight.foreground3": "#f99157",
    "editorBracketHighlight.foreground4": "#ec5f67",
    "editorBracketHighlight.foreground5": "#bb80b3",
    "editorBracketHighlight.foreground6": "#98C379"
  },
  "editor.semanticTokenColorCustomizations": {
    "enabled": true
  },
  "editor.tokenColorCustomizations": {
    "[Kanagawa]": {
      "comments": {}
    },
    "textMateRules": [
      {
        "scope": [
          "keyword.control",
          "entity.name.type",
          "entity.name.section",
          "entity.name.tag",
          "constant"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "entity.name.function",
          "entity.name.method",
          "entity.name.class",
          "meta.class"
        ],
        "settings": {
          "fontStyle": "bold "
        }
      },
      {
        "scope": [
          "comment",
          "comment.block",
          "comment.block.documentation",
          "comment.line"
        ],
        "settings": {
          "fontStyle": "italic underline"
        }
      },
      {
        "scope": ["invalid.deprecated"],
        "settings": {
          "fontStyle": "bold strikethrough"
        }
      }
    ]
  },
  "workbench.iconTheme": "catppuccin-macchiato",
  "explorer.confirmDelete": false,
  "[markdown]": {
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.wordWrap": "on",
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    },
    "editor.formatOnSave": true
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "go.toolsManagement.autoUpdate": true,
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[html]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "telemetry.telemetryLevel": "off",
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "prettier.bracketSpacing": true,
  "prettier.arrowParens": "always",
  "prettier.embeddedLanguageFormatting": "auto",
  "eslint.run": "onType",
  "eslint.format.enable": true,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "redhat.telemetry.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "editor.stickyScroll.enabled": true,
  "terminal.integrated.smoothScrolling": true,
  "editor.inlayHints.enabled": "on",
  "python.languageServer": "Pylance",
  "python.analysis.typeCheckingMode": "basic",
  "autoDocstring.docstringFormat": "numpy",
  "autoDocstring.guessTypes": true,
  "go.vetOnSave": "package",
  "go.lintOnSave": "file",
  "go.buildOnSave": "package",
  "go.lintTool": "golangci-lint",
  "go.lintFlags": ["--fast"],
  "workbench.startupEditor": "none",
  // Python stuff
  "python.terminal.activateEnvironment": true,
  "indentRainbow.includedLanguages": [
    "python",
    "yaml",
    "toml",
    "Dockefile",
    "yml"
  ],
  "indentRainbow.indicatorStyle": "light",
  "indentRainbow.colors": [
    "rgba(255, 0 , 0,0.5)",
    "rgba(255, 127, 0,0.5)",
    "rgba(255, 255, 0,0.5)",
    "rgba(0, 255, 0,0.5)",
    "rgba(0, 100, 255,0.5)",
    "rgba(100, 0, 252,0.5)",
    "rgba(250,0,211,0.5)"
  ],
  "better-comments.multilineComments": true, // multiline comments will highlight all lines
  "better-comments.tags": [
    {
      "tag": "!", // ! example
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?", // ? example
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//", // // example
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo", // todo example
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*", // * example
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.autoSearchPaths": true,
  "python.analysis.useLibraryCodeForTypes": true,
  "python.analysis.autoImportCompletions": true,
  "python.interpreter.infoVisibility": "onPythonRelated",
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": true,
      "ruff.organizeImports": true
    }
  },
  "cSpell.enableFiletypes": ["md"],
  "cSpell.enabledLanguageIds": ["markdown"],
  "yaml.schemas": {},
  "[sql]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "mtxr.sqltools"
  },
  "javascript.autoClosingTags": true,
  "typescript.autoClosingTags": true,
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "json.schemas": [],
  "editor.inlineSuggest.enabled": true,
  "git.ignoreMissingGitWarning": true,
  "jupyter.askForKernelRestart": false,
  "editor.linkedEditing": true,
  "workbench.preferredDarkColorTheme": "Kanagawa",
  "workbench.preferredLightColorTheme": "Horizon Extended Light",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": true,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "security.workspace.trust.untrustedFiles": "open",
  "rust-analyzer.cargo.autoreload": true,
  "rust-analyzer.completion.autoimport.enable": true,
  "rust-analyzer.diagnostics.enable": true,
  "rust-analyzer.checkOnSave": true,
  "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "skip_trivial",
  "rust-analyzer.inlayHints.reborrowHints.enable": "never",
  "rust-analyzer.inlayHints.typeHints.enable": true,
  "rust-analyzer.check.command": "clippy",
  "sonarlint.rules": {
    "python:S5886": {
      "level": "off"
    },
    "python:S1515": {
      "level": "off"
    }
  },
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "editor.cursorSmoothCaretAnimation": "on",
  "diffEditor.codeLens": true,
  "[dockercompose]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vs-kubernetes": {
    "vs-kubernetes.minikube-show-information-expiration": "2023-08-07T19:23:04.972Z"
  },
  "git.openRepositoryInParentFolders": "never",
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": true
  },
  "sqltools.connections": [],
  "git.confirmSync": false,
  "python.analysis.extraPaths": [],
  "svelte.enable-ts-plugin": true,
  "workbench.colorTheme": "Tokyo Night",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "githubPullRequests.pullBranch": "never"
}
